{
    "name": "js-sprint",
    "private": true,
    "description": "тренировочние задание",
    "scripts": {
        "editorconfig": "editorconfig-cli",
        "stylelint": "stylelint \"src/styles/**/*.scss\" --syntax scss",
        "test": "npm run editorconfig && npm run stylelint",
        "build:dev": "cross-env NODE_ENV=development webpack --mode development",
        "build:prod": "cross-env NODE_ENV=production webpack --mode production",
        "start:dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --stats-children",
        "start:prod": "cross-env NODE_ENV=production webpack-dev-server --mode production --open"
    },
    "devDependencies": {
        "@babel/core": "7.18.6",
        "@babel/polyfill": "7.12.1",
        "@babel/preset-env": "7.18.6",
        "@htmlacademy/editorconfig-cli": "1.0.0",
        "babel-loader": "8.2.5",
        "clean-webpack-plugin": "4.0.0",
        "cross-env": "7.0.3",
        "css-loader": "6.7.1",
        "del": "6.0.0",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.5.0",
        "mini-css-extract-plugin": "2.6.1",
        "node-sass": "7.0.1",
        "optimize-css-assets-webpack-plugin": "6.0.1",
        "sass": "1.52.3",
        "sass-loader": "13.0.2",
        "style-loader": "3.3.1",
        "stylelint": "13.12.0",
        "stylelint-config-htmlacademy": "0.1.4",
        "terser-webpack-plugin": "5.3.3",
        "webpack": "5.73.0",
        "webpack-cli": "4.10.0",
        "webpack-dev-server": "4.9.3"
    },
    "browserslist": [
        "last 10 versions",
        "not dead",
        "not ie <= 11"
    ],
    "editorconfig-cli": [
        "*.json",
        "*.js",
        "src/*.html",
        "src/js/**/*.js",
        "src/assets/**/*.svg",
        "src/styles/**/*.scss"
    ],
    "engines": {
        "node": ">=14.15.0"
    },
    "dependencies": {}
}
